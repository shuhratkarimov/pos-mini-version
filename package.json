{
  "name": "imkonqurilish",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "electron-main.js",
  "scripts": {
    "dev": "vite",
    "start": "electron .",
    "build:vite": "vite build",
    "build:netlify": "npm run build:vite",
    "build": "npm run build:vite && electron-builder",
    "electron:dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\""
  },
  "build": {
    "appId": "com.mycompany.myapp",
    "productName": "imkonqurilish-santexnika",
    "files": [
      "dist/**",
      "electron-main.js",
      "package.json"
    ],
    "directories": {
      "buildResources": "public"
    },
    "win": {
      "target": "nsis",
      "icon": "icons/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "icons/icon.icns"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "icons/icon.png"
    }
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
